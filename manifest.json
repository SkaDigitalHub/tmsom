{
  "id": "/",
  "scope": "/",
  "name": "TMSOM App",
  "display": "standalone",
  "start_url": "/",
  "short_name": "tmsom",
  "theme_color": "#8A1878",
  "description": "Comprehensive platform for TMSOM education access",
  "orientation": "any",
  "background_color": "#008000",
  "related_applications": [],
  "dir": "ltr",
  "lang": "en",
  "prefer_related_applications": false,
  "display_override": ["window-controls-overlay"],
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "icons": [
    {
      "src": "logo/main.png",
      "sizes": "512x512",
      "type": "image/png"
    },
    {
      "src": "logo/main.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "logo/main.png",
      "sizes": "48x48",
      "type": "image/png"
    },
    {
      "src": "logo/main.png",
      "sizes": "24x24",
      "type": "image/png"
    }
  ],
  "screenshots": [
    {
      "src": "assets/img/screen1.png",
      "sizes": "1617x1012",
      "type": "image/png"
    },
    {
      "src": "assets/img/screen2.png",
      "sizes": "750x1334",
      "type": "image/png",
      "form_factor": "narrow"
    }
  ],
  "features": [
    "Cross Platform",
    "fast",
    "simple"
  ],
  "categories": [
    "education"
  ],
  "shortcuts": [
    {
      "name": "Open AI Agent",
      "short_name": "Agent",
      "description": "Open the AI Agent page",
      "url": "/agent",
      "icons": [{ "src": "logo/agent.png", "sizes": "192x192" }]
    },
    {
      "name": "Log in",
      "short_name": "Login",
      "url": "/login",
      "description": "Start a new session"
    }
  ],
  "author": {
    "name": "SkaDigital Hub",
    "website": "https://skadigitalhub.github.io/SkaApps"
  },
  "share_target": {
    "action": "/share",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "file_handlers": [{
    "action": "/pdf-viewer",
    "accept": {
      "application/pdf": [".pdf"],
      "application/octet-stream": [".pdf"],
      "application/vnd.pdf": [".pdf"]
    }
  }],
  "protocol_handlers": [{
    "protocol": "web+tmsom",
    "url": "/tmsom/?url=%s"
  }]
}
