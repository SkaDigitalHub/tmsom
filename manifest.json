{
  "id": "/tmsom/",
  "scope": "/tmsom/",
  "name": "TMSOM App",
  "display": "standalone",
  "start_url": "/tmsom/",
  "short_name": "TMSOM",
  "theme_color": "#8A1878",
  "description": "Comprehensive platform for TMSOM education access",
  "orientation": "any",
  "background_color": "#008000",
  "related_applications": [],
  "dir": "ltr",
  "lang": "en",
  "prefer_related_applications": false,
  "display_override": ["window-controls-overlay"],
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "icons": [
    {
      "src": "/tmsom/logo/main.png",
      "sizes": "512x512",
      "type": "image/png"
    },
    {
      "src": "/tmsom/logo/main.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/tmsom/logo/main.png",
      "sizes": "48x48",
      "type": "image/png"
    },
    {
      "src": "/tmsom/logo/main.png",
      "sizes": "24x24",
      "type": "image/png"
    }
  ],
  "screenshots": [
    {
      "src": "/tmsom/assets/img/screen1.png",
      "sizes": "1617x1012",
      "type": "image/png",
      "platform": "wide"
    },
    {
      "src": "/tmsom/Screenshot.jpg",
      "sizes": "750x1334",
      "type": "image/jpg",
      "form_factor": "narrow"
    }
  ],
  "features": [
    "Cross Platform",
    "fast",
    "simple"
  ],
  "categories": [
    "education"
  ],
  "shortcuts": [
    {
      "name": "Open AI Agent",
      "short_name": "Agent",
      "description": "Open the AI Agent page",
      "url": "/tmsom/agent",
      "icons": [{ "src": "/tmsom/logo/agent.icon.png", "sizes": "192x192" }]
    },
    {
      "name": "Log in",
      "short_name": "Login",
      "url": "/tmsom/login",
      "description": "Start a new session"
    }
  ],
  "author": {
    "name": "SkaDigital Hub",
    "website": "https://skadigitalhub.github.io/SkaApps"
  },
  "share_target": {
    "action": "/tmsom/share",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "file_handlers": [{
    "action": "/tmsom/pdf-viewer",
    "accept": {
  "application/pdf": [".pdf"],
  "text/plain": [".txt"],
  "application/vnd.openxmlformats-officedocument.wordprocessingml.document": [".docx"]
    }
  }],
  "protocol_handlers": [{
    "protocol": "web+tmsom",
    "url": "/tmsom/?url=%s"
  }],
  "edge_side_panel": {
  "preferred_width": 340
},
"handle_links": "preferred"
}
